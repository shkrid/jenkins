pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
                docker version
            }
        }
        stage('Dockerize') {
            steps {
                echo 'Testing..'
                docker version
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
                docker version
            }
        }
    }
}
